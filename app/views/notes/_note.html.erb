<tr>
  <td><%= link_to "#{note.folio}", note %></td>
  <td><%= link_to "#{t('patient.cgdvcode')}: #{cgdvcode(note)}", note %></td>
  <td><%= l note.created_at, :format => :small %></td>
  <% if can? :delete, @note %>
  <td><%= link_to t('helpers.delete.msg'), note, :method => :delete, :confirm => t('helpers.delete.conf'), :title => "#{t('helpers.delete.msg')} #{note.folio}" %></td>
  <% end %>
  <td><%= link_to t('helpers.print'), print_note_path(note) %></td>
</tr>

