<tr align="left" class="noteprint-patient-row">
  <th width="129" align="center" class="noteprint-patient-label"><%= t('patient.name') %>:</th>
  <td width="466" align="center" class="noteprint-patient-value"><%= name(@note) %></td>
  <th width="117" align="center" class="noteprint-patient-label">Registro</th>
  <td width="98" align="center" class="noteprint-patient-value"><%= cgdvcode(@note) %></td>
</tr>
<tr align="center" class="noteprint-columns-row">
  <td><strong><%= t('note.codigo') %></strong></td>
  <td><strong><%= t('note.descripcion') %></strong></td>
  <td><strong><%= t('note.cuota') %></strong></td>
  <td><strong><%= t('note.cantidad') %></strong></td>
</tr>
<% for element in @note.elements %>  
<tr class="noteprint-item-row">
  <td align="center"><%= element.codigo %></td>
  <td align="left"><%= element.descripcion %></td>
  <td align="center"><%= number_to_currency(element.cuota) %></td>
  <td align="center"><%= element.cantidad %></td>
</tr>
<% end %>
<tr align="center" class="noteprint-subtotal-row">
  <th CELLPADDING=10 colspan="3" align="right"><%= t('note.subtotal') %></th>
  <td><%= number_to_currency(@note.subtotal) %></td>
</tr>
