<tr>
  <td><%= link_to cgdvcode_vol(timereport), volunteer_path(timereport.volunteer_id) %></td>
  <td><%= link_to l(timereport.day), timereport %></td>
  <td><%= l timereport.begin, :format => :hourmin %></td>
  <td><%= l timereport.end, :format => :hourmin %></td>
  <td><%= link_to timereport.tiempo, timereport %></td>
  <td><%= timereport.evento %></td>
  <td class="smally" colspan=3><%= link_to t('helpers.edit'), edit_timereport_path(timereport) %></td>
  <% if can? :destroy, timereport %>
  <td class="smally" colspan=3>
  	<%= link_to t('helpers.delete.msg'), timereport_path(timereport), :method => :delete, :confirm => t('helpers.delete.conf'), :title => "#{t('helpers.delete.msg')} #{timereport.begin}" %>
  </td>
  <% end %>
</tr>	